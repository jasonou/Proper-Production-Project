{
  "name": "backend-starter",
  "version": "1.0.0",
  "description": "Backend Starter",
  "scripts": {
    "prelint": "eslint  . --fix",
    "lint": "eslint .",
    "start": "node --optimize-for-size --max-old-space-size=460 --gc-interval=100 ./bin/start.js",
    "start:inspect": "node --optimize-for-size --max-old-space-size=460 --gc-interval=100 --inspect-brk=0.0.0.0:9229 ./bin/start.js",
    "dev": "npm run lint && npm run start",
    "dev:inspect": "npm run lint && npm run start:inspect"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "connect-redis": "4.0.4",
    "express": "4.17.1",
    "express-rate-limit": "5.1.1",
    "express-session": "1.17.0",
    "helmet": "3.21.2",
    "morgan": "1.9.1",
    "redis": "3.0.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0"
  }
}
